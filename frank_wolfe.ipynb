    "    x = x0\n",
    "    while(True):\n",
    "        alfa = 2/(k+2)\n",
    "        fun = lambda s: s*df(x) #skalarno\n",
    "        s = minimize(fun,x)\n",
    "        x = x + alfa(s-x)\n",
    "        k+=1\n",
    "        "
